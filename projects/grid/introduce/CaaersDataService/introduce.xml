  <ns16:Property description="" isFromETC="false" key="cqlQueryProcessorConfig_appserviceUrl" value="http://localhost:8080/cacore31/server/HTTPServer"/>
  <ns16:Property description="" isFromETC="false" key="cqlQueryProcessorConfig_useCsmSecurity" value="false"/>
  <ns16:Property description="" isFromETC="false" key="cqlQueryProcessorConfig_gridGrouperUrl" value="https://dorian.org2.org:8447/wsrf/services/cagrid/GridGrouper"/>
