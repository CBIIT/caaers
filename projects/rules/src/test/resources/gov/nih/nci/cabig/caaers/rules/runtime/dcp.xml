<?xml version="1.0" encoding="UTF-8" standalone="yes"?><ruleSet xmlns="http://caaers.cabig.nci.nih.gov/rules/brxml"><id>988c47b7-a73d-426f-987e-8be4ad39e501</id><name>gov.nih.nci.cabig.caaers.rules.sponsor.division_of_cancer_prevention.sae_reporting_rules</name><description>SAE Reporting Rules</description><subject>Sponsor rules||Division of Cancer Prevention</subject><coverage>Enabled</coverage><import>gov.nih.nci.cabig.caaers.domain.*</import><rule id="b1397955-9608-438e-9143-bec105fc7a49"><condition><column display-uri="Study" object-type="gov.nih.nci.cabig.caaers.domain.Study" identifier="study"><field-constraint grammer-postfix="" grammer-prefix=" AdEERS Reporting " display-uri="undefined" field-name="adeersReporting"><literal-restriction display-uri="is " evaluator="=="><value>false</value><readable-value>No </readable-value></literal-restriction></field-constraint><expression>factResolver.assertFact(study,null,'adeersReporting','runTimeValue','runTimeOperator')</expression></column><column display-uri="Study" object-type="gov.nih.nci.cabig.caaers.domain.Study" identifier="study"><field-constraint grammer-postfix="" grammer-prefix=" Study " display-uri="undefined" field-name="phaseCode"><literal-restriction display-uri="is " evaluator="=="><value>Phase I Trial</value><value>Phase I/II Trial</value><value>Phase II Trial</value><readable-value>Phase II Trial,Phase I/II Trial,Phase I Trial,</readable-value></literal-restriction></field-constraint><expression>factResolver.assertFact(study,null,'phaseCode','runTimeValue','runTimeOperator')</expression></column><column display-uri="Expedited Adverse Event Report" object-type="gov.nih.nci.cabig.caaers.domain.ExpeditedAdverseEventReport" identifier="aeReport"><field-constraint grammer-postfix="" grammer-prefix=" Outcome Identifier " display-uri="undefined" field-name="outcomeIdentifier"><literal-restriction display-uri="is " evaluator="=="><value>1</value><value>2</value><value>3</value><value>4</value><value>5</value><value>6</value><value>7</value><readable-value>required intervention to prevent permanent impairment/damage (Devices) ,other serious (important medical events) ,congenital anomaly/birth defect,disability or permanent damage,hospitalization - intial or prolonged ,life-threatening,death,</readable-value></literal-restriction></field-constraint><expression>factResolver.assertFact(aeReport,'gov.nih.nci.cabig.caaers.domain.OutcomeType','code','runTimeValue','runTimeOperator')</expression></column><column object-type="gov.nih.nci.cabig.caaers.rules.objectgraph.FactResolver" identifier="factResolver"><field-constraint><literal-restriction/></field-constraint></column><column object-type="gov.nih.nci.cabig.caaers.domain.Study" identifier="studySDO"><field-constraint field-name="primarySponsorCode"><literal-restriction evaluator="=="><value>Division of Cancer Prevention</value></literal-restriction></field-constraint><expression>factResolver.assertFact(studySDO,"gov.nih.nci.cabig.caaers.domain.Organization","name","Division of Cancer Prevention","==")</expression></column></condition><action>dcp rd</action><readableAction>dcp rd</readableAction><metaData checkinComment="Initial Version" packageName="gov.nih.nci.cabig.caaers.rules.sponsor.division_of_cancer_prevention.sae_reporting_rules" description="Setting Description since its mandatory by JBoss Repository config" name="Rule-1"><category><path>CAAERS_BASE/SPONSOR/division_of_cancer_prevention</path><metaData name="sae_reporting_rules"/></category></metaData><readableRule><line>IF</line><line>	 &amp;nbsp;&amp;nbsp;&amp;nbsp; AdEERS Reporting is  'No ' </line><line>AND</line><line>	 &amp;nbsp;&amp;nbsp;&amp;nbsp; Study is  'Phase II Trial'  or 'Phase I/II Trial'  or 'Phase I Trial' </line><line>AND</line><line>	 &amp;nbsp;&amp;nbsp;&amp;nbsp; Outcome Identifier is  'required intervention to prevent permanent impairment/damage (Devices) '  or 'other serious (important medical events) '  or 'congenital anomaly/birth defect'  or 'disability or permanent damage'  or 'hospitalization - intial or prolonged '  or 'life-threatening'  or 'death' </line></readableRule></rule></ruleSet>