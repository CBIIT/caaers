<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="expedited_domestic">


	<description>
		Expedited report flow:
   (domestic) Site CRA (entry) -&gt; Physician (review) -&gt; Site CRA (submit) -&gt; CALGB Central Office (review) -&gt; Submit to AdEERS
	</description>


	<start-state name="Start Expedited Domestic Flow">
		<transition to="Submit Report To Physician" name="start"></transition>
	</start-state>


	<node name="Submit Report To Physician">
		<action name="nodeSkipActionHandler" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
			<targetBean>
				nodeSkipActionHandler
			</targetBean>
			<factoryKey>
				jbpmConfiguration
			</factoryKey>
		</action>
		<description>
			This task is assigned to the CRA for Data entry associated to the report.
		</description>
		<event type="node-leave">
			<action class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" name="taskCloseActionHandler" config-type="bean">
				<targetBean>
					taskCloseActionHandler
				</targetBean>
				<factoryKey>
					jbpmConfiguration
				</factoryKey>
			</action>
		</event>
		<transition to="Physician Review" name="Send to Physician for Review"></transition>
		<transition to="Central Office SAE Coordinator Review" name="Submit to Central Office SAE Coordinator"></transition>
	</node>

	<node name="Physician Review">
		<action name="nodeSkipActionHandler" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
			<targetBean>
				nodeSkipActionHandler
			</targetBean>
			<factoryKey>
				jbpmConfiguration
			</factoryKey>
		</action>
		<description>
			This task is assigned to the Physican, to review the report submitted by the CRA. The Physican may approve the report (sign off) or will request additional information. 
		</description>
		<event type="node-leave">
			<action class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" name="taskCloseActionHandler" config-type="bean">
				<targetBean>
					taskCloseActionHandler
				</targetBean>
				<factoryKey>
					jbpmConfiguration
				</factoryKey>
			</action>
		</event>		
		<transition to="Submit Report To Central Office" name="Approve Report"></transition>
		<transition to="Provide Additional Information To Physician" name="Request Additional Information"></transition>
	</node>

	<node name="Submit Report To Central Office">
		<action name="nodeSkipActionHandler" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
			<targetBean>
				nodeSkipActionHandler
			</targetBean>
			<factoryKey>
				jbpmConfiguration
			</factoryKey>
		</action>
		<description>
			This task is assigned to the CRA, the Physican has approved the report. The CRA now have to submit the report to Central office for further review. 
		</description>
		<event type="node-leave">
			<action class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" name="taskCloseActionHandler" config-type="bean">
				<targetBean>
					taskCloseActionHandler
				</targetBean>
				<factoryKey>
					jbpmConfiguration
				</factoryKey>
			</action>
		</event>		
		<transition to="Central Office SAE Coordinator Review" name="Submit to Central Office SAE Coordinator"></transition>
	</node>

	<node name="Provide Additional Information To Physician">
		<action name="nodeSkipActionHandler" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
			<targetBean>
				nodeSkipActionHandler
			</targetBean>
			<factoryKey>
				jbpmConfiguration
			</factoryKey>
		</action>
		<description>
			This task is assigned to the CRA, the Physican has request for additional information. The CRA has to provide that information and submit the report to Physican for review
		</description>
		<event type="node-leave">
			<action class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" name="taskCloseActionHandler" config-type="bean">
				<targetBean>
					taskCloseActionHandler
				</targetBean>
				<factoryKey>
					jbpmConfiguration
				</factoryKey>
			</action>
		</event>		
		<transition to="Physician Review" name="Send to Physician for Review"></transition>
	</node>

	<node name="Central Office SAE Coordinator Review">
		<action name="nodeSkipActionHandler" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
			<targetBean>
				nodeSkipActionHandler
			</targetBean>
			<factoryKey>
				jbpmConfiguration
			</factoryKey>
		</action>
		<description>
			This taks is associated to the Personnel at Central office. The central office personal may approve the report, of will send it back to the submitter (CRA) asking additional information. 
		</description>
		<event type="node-leave">
			<action class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" name="taskCloseActionHandler" config-type="bean">
				<targetBean>
					taskCloseActionHandler
				</targetBean>
				<factoryKey>
					jbpmConfiguration
				</factoryKey>
			</action>
		</event>	
		<transition to="Provide Additional Information To Central Office" name="Request Additional Information"></transition>
		<transition to="End Expedited Domestic Flow" name="Approve Report"></transition>
		
	</node>

	<node name="Provide Additional Information To Central Office">
		<action name="nodeSkipActionHandler" class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" config-type="bean">
			<targetBean>
				nodeSkipActionHandler
			</targetBean>
			<factoryKey>
				jbpmConfiguration
			</factoryKey>
		</action>
		<description>
			This task is associated to the CRA, the central office has requested for additional information. The CRA must complete the information and send it to Central office for further review. 
		</description>
		<event type="node-leave">
			<action class="org.springmodules.workflow.jbpm31.JbpmHandlerProxy" name="taskCloseActionHandler" config-type="bean">
				<targetBean>
					taskCloseActionHandler
				</targetBean>
				<factoryKey>
					jbpmConfiguration
				</factoryKey>
			</action>
		</event>		
		<transition to="Central Office SAE Coordinator Review" name="Submit to Central Office SAE Coordinator"></transition>
		<transition to="Physician Review" name="Send to Physician for Review"></transition>
	</node>


	<end-state name="End Expedited Domestic Flow"></end-state>


</process-definition>