<%@taglib prefix="tags" tagdir="/WEB-INF/tags"%>
<%@taglib prefix="ae" tagdir="/WEB-INF/tags/ae"%>
<%@ include file="/WEB-INF/views/taglibs.jsp"%>
<tags:noform>
	<div>
		<div class="eXtremeTable" >
		<c:forEach items="${command.previousComments}" var="comment" varStatus="cIndex">
					<div class="${cIndex.index % 2 gt 0 ? 'odd' : 'even' } autoclear cmt" >
						<c:set var="dt"><tags:formatDate value="${comment.createdDate}" /></c:set>
						<c:if test="${comment.editable == true}">
							<chrome:division title='${comment.autoGeneratedText } on ${dt } <a href="javascript:editComment(${comment.id})">(edit)</a>' collapsable="true" id="cmt${cIndex.index}" collapsed="${cIndex.index gt 0}">
						</c:if>
						<c:if test="${comment.editable == false}">
							<chrome:division title='${comment.autoGeneratedText } on ${dt }' collapsable="true" id="cmt${cIndex.index}" collapsed="${cIndex.index gt 0}">
						</c:if>
						<pre id="userComment-${comment.id}">${comment.userComment}</pre>
						</chrome:division>
						
					</div>
				</c:forEach>
			</div>
	</div>
</tags:noform>