<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://schema.integration.caaers.cabig.nci.nih.gov/aereport"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://schema.integration.caaers.cabig.nci.nih.gov/aereport"
	xmlns:cct="http://schema.integration.caaers.cabig.nci.nih.gov/common"
	elementFormDefault="qualified" attributeFormDefault="unqualified">

	<xs:import namespace="http://schema.integration.caaers.cabig.nci.nih.gov/common" schemaLocation="caAERSCommonTypes.xsd" />
		<xs:element name="AdverseEventReport">
			 <xs:complexType>
				<xs:sequence>
					<xs:element type="xs:string" name="createdAt" />
					<xs:element type="AdverseEventResponseDescriptionType" name="adverseEventResponseDescription" />
					<xs:element type="AdverseEventReportingPeriodType" name="adverseEventReportingPeriod" />
					<xs:element type="PhysicianType" name="physician" />
					<xs:element type="ReporterType" name="reporter" />
					<xs:element type="ReviewerType" name="reviewer" />
					<xs:element type="DiseaseHistoryType" name="diseaseHistory"  minOccurs="0"  />
					<xs:element type="ParticipantHistoryType" name="participantHistory" minOccurs="0"  />
					<xs:element type="StudyParticipantAssignmentType" name="studyParticipantAssignment" minOccurs="0"  />
					<xs:element type="RadiationInterventionType" name="radiationIntervention" minOccurs="0"  />
					<xs:element type="SurgeryInterventionType" name="surgeryIntervention" minOccurs="0"  />
					<xs:element type="BiologicalInterventionType" name="biologicalIntervention"  minOccurs="0" />
					<xs:element type="DietarySupplementInterventionType" name="dietarySupplementIntervention" minOccurs="0"  />
					<xs:element type="MedicalDeviceType" name="medicalDevice" minOccurs="0" />
					<xs:element type="ConcomitantMedicationType" name="concomitantMedication" maxOccurs="unbounded" minOccurs="0" />
					<xs:element type="AdverseEventType" name="adverseEvent"	maxOccurs="unbounded" minOccurs="0" />
					<xs:element type="LabType" name="lab" />
					<xs:element type="SAEReportPriorTherapyType" name="sAEReportPriorTherapy" maxOccurs="unbounded" minOccurs="0" />
					<xs:element type="TreatmentInformationType" name="treatmentInformation" minOccurs="0"  />
					<xs:element type="SAEReportPreExistingConditionType" name="sAEReportPreExistingCondition" maxOccurs="unbounded" minOccurs="0" />
					<xs:element type="OtherCauseType" name="otherCause"	maxOccurs="unbounded" minOccurs="0" />
					<xs:element type="AdditionalInformationType" name="additionalInformation" minOccurs="0" />
					<xs:element type="ReportType" name="report" />
				</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:complexType name="TimeValueType">
		<xs:sequence>
			<xs:element type="xs:integer" name="hour" minOccurs="0"/>
			<xs:element type="xs:integer" name="minute" minOccurs="0"/>
			<xs:element type="xs:int" name="type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdverseEventResponseDescriptionType">
		<xs:sequence>
			<xs:element type="xs:string" name="eventDescription" />
			<xs:element type="xs:string" name="presentStatus" />
			<xs:element type="xs:boolean" name="retreated" />
			<xs:element name="recoveryDate" type="xs:date"/>
			<xs:element type="xs:date" name="dateRemovedFromProtocol" />
			<xs:element type="xs:string" name="eventAbate" />
			<xs:element type="xs:string" name="eventReappear" />
			<xs:element type="TimeValueType" name="primaryTreatmentApproximateTime" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdverseEventReportingPeriodType">
		<xs:sequence>
			<xs:element type="xs:integer" name="cycleNumber" />
			<xs:element type="xs:date" name="startDate" />
			<xs:element type="StudyParticipantAssignmentType" name="StudyParticipantAssignment" />
			<xs:element type="TreatmentAssignmentType" name="TreatmentAssignment" />
		</xs:sequence>
	</xs:complexType>
	
	 <xs:complexType name="ContactMechanismType">
	  <xs:sequence>
  		<xs:element name="type" type="xs:string"/>
  		<xs:element name="value" type="xs:string"/>
	  </xs:sequence>
  	</xs:complexType> 
	
	<xs:complexType name="PhysicianType">
		<xs:sequence>
			<xs:element type="xs:string" name="firstName" />
			<xs:element type="xs:string" name="lastName" />
			<xs:element type="xs:string" name="nciIdentifier" minOccurs="0"/>
			<xs:element type="xs:string" name="address" minOccurs="0" />
			<xs:element type="ContactMechanismType" name="ContactMechanism" maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="ReporterType">
		<xs:sequence>
			<xs:element type="xs:string" name="firstName" />
			<xs:element type="xs:string" name="lastName" />
			<xs:element type="xs:string" name="nciIdentifier" minOccurs="0"/>
			<xs:element type="xs:string" name="address" minOccurs="0" />
			<xs:element type="ContactMechanismType" name="ContactMechanism"	maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="ReviewerType">
		<xs:sequence>
			<xs:element type="xs:string" name="firstName" />
			<xs:element type="xs:string" name="lastName" />
			<xs:element type="xs:string" name="nciIdentifier" minOccurs="0"/>
			<xs:element type="xs:string" name="address" minOccurs="0" />
			<xs:element type="ContactMechanismType" name="ContactMechanism"	maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="AnatomicSiteType">	  
	  <xs:sequence>
	  	<xs:element name="name" type="xs:string"/>
	  	<xs:element name="category" type="xs:string"/>
	  </xs:sequence>
   </xs:complexType>
	
	<xs:complexType name="MetastaticDiseaseSiteType">
	  <xs:sequence>
	 		<xs:element name="otherMetastaticDiseaseSite" type="xs:string"/>
	 		<xs:element name="AnatomicSite" type="AnatomicSiteType"/>
	  </xs:sequence>
	</xs:complexType>  

	<xs:complexType name="DiseaseHistoryType">
		<xs:sequence>
			<xs:element type="xs:string" name="diagnosisDate" />
			<xs:element type="MetastaticDiseaseSiteType" name="metastaticDiseaseSite" maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="HeightType">
		<xs:sequence>
			<xs:element type="xs:float" name="quantity" />
			<xs:element type="xs:string" name="unit" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="WeightType">
		<xs:sequence>
			<xs:element type="xs:float" name="quantity" />
			<xs:element type="xs:string" name="unit" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="AgentType">
	  <xs:sequence>
  		<xs:element name="name" type="xs:string"/>
  		<xs:element name="description" type="xs:string"/>
  		<xs:element name="nscNumber" type="xs:string"/>
	  </xs:sequence>
  </xs:complexType>
	<xs:complexType name="StudyAgentType">
		<xs:sequence>
			<xs:element name="Agent" type="AgentType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DiseaseTermType">
        <xs:complexContent>
            <xs:extension base="cct:MutableDomainObjectType">
                <xs:sequence>
                    <xs:element name="term" type="cct:CustomStringType" minOccurs="0"/>
                    <xs:element name="meddraCode" type="cct:CustomStringType" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="StudyType">
        <xs:complexContent>
            <xs:extension base="cct:MutableDomainObjectType">
                <xs:sequence>
                    <xs:element name="identifiers" minOccurs="1" maxOccurs="1">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="identifier" type="ReducedIdentifierType"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
     <xs:complexType name="ReducedIdentifierType">
        <xs:complexContent>
            <xs:extension base="cct:MutableDomainObjectType">
                <xs:sequence>
                    <xs:element name="type" type="cct:StudyIdentifierType"/>
                    <xs:element name="value" type="xs:string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:element name="organizationAssignedIdentifier" type="OrganizationAssignedIdentifierType"/>
    <xs:complexType name="OrganizationAssignedIdentifierType">
        <xs:complexContent>
            <xs:extension base="IdentifierType">
                <xs:sequence>
                    <xs:element ref="organizationRef"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:element name="organizationRef" type="OrganizationRefType"/>
    <xs:complexType name="OrganizationRefType">
        <xs:sequence>
            <xs:element name="nciInstituteCode" type="cct:CustomStringType" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>

	<xs:element name="systemAssignedIdentifier" type="SystemAssignedIdentifierType"/>
    <xs:complexType name="SystemAssignedIdentifierType">
        <xs:complexContent>
            <xs:extension base="IdentifierType">
                <xs:sequence>
                    <xs:element name="systemName" type="cct:CustomStringType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="IdentifierType">
        <xs:complexContent>
            <xs:extension base="cct:MutableDomainObjectType">
                <xs:sequence>
                    <xs:element name="type" type="cct:ParticipantIdentifierType"/>
                    <xs:element name="value" type="cct:CustomStringType"/>
                    <xs:element name="primaryIndicator" type="xs:boolean" />
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
	<xs:complexType name="StudySiteType">
		<xs:sequence>
			<xs:element type="xs:string" name="statusCode" minOccurs="0"/>
			<xs:element type="StudyType" name="Study"/>
			<xs:element type="cct:OrganizationType" name="Organization" />
		</xs:sequence>
	</xs:complexType>
	
	 <xs:simpleType name="GenderType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Male"/>
            <xs:enumeration value="Female"/>
            <xs:enumeration value="Not Reported"/>
            <xs:enumeration value="Unknown"/>
        </xs:restriction>
    </xs:simpleType> 
    
    <xs:simpleType name="EthnicityType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Hispanic or Latino"/>
            <xs:enumeration value="Not Hispanic or Latino"/>
            <xs:enumeration value="Not Reported"/>
            <xs:enumeration value="Unknown"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="FirstAndLastNameType">
        <xs:restriction base="xs:normalizedString">
        	<xs:minLength value="1"/>
        	<xs:pattern value="[a-zA-Z0-9_ ]*"></xs:pattern>
            <xs:whiteSpace value="collapse"/> 
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="MiddleAndMaidenNameType">
        <xs:restriction base="xs:normalizedString">
        	<xs:pattern value="[a-zA-Z0-9_ ]*"></xs:pattern>
            <xs:whiteSpace value="collapse"/> 
        </xs:restriction>
    </xs:simpleType>
    
    
    <xs:simpleType name="RaceType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Asian"/>
            <xs:enumeration value="White"/>
            <xs:enumeration value="Black or African American"/>
            <xs:enumeration value="American Indian or Alaska Native"/>
            <xs:enumeration value="Native Hawaiian or Other PacificIslander"/>
            <xs:enumeration value="Not Reported"/>
            <xs:enumeration value="Unknown"/>
        </xs:restriction>
    </xs:simpleType>
	
	<xs:complexType name="ParticipantType">
		<xs:sequence>
			<xs:element name="firstName" type="FirstAndLastNameType" minOccurs="1" maxOccurs="1" />
			<xs:element name="lastName" type="FirstAndLastNameType" minOccurs="1" maxOccurs="1" />
			<xs:element name="maidenName" type="MiddleAndMaidenNameType" minOccurs="0" />
			<xs:element name="middleName" type="MiddleAndMaidenNameType" minOccurs="0" />
			<xs:choice>
				<xs:element name="birthDate" type="xs:date" />
				<xs:sequence>
					<xs:element name="birthMonth" type="cct:CustomMonthType" minOccurs="1" maxOccurs="1" />
					<xs:element name="birthYear" type="cct:CustomDateType" minOccurs="1" maxOccurs="1" />
				</xs:sequence>
			</xs:choice>
			<xs:element name="gender" type="GenderType" minOccurs="1" maxOccurs="1" />
			<xs:element name="race" type="RaceType" minOccurs="1" maxOccurs="1" />
			<xs:element name="ethnicity" type="EthnicityType" minOccurs="1" maxOccurs="1" />

			<xs:element name="identifiers" minOccurs="1" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="organizationAssignedIdentifier" minOccurs="1" maxOccurs="unbounded" />
						<xs:element ref="systemAssignedIdentifier" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StudyParticipantAssignmentType">
		<xs:sequence>
			<xs:element type="xs:date" name="dateOfEnrollment" />
			<xs:element type="StudySiteType" name="StudySite" />
			<xs:element type="ParticipantType" name="Participant" />
			<xs:element type="xs:string" name="studySubjectIdentifier" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RadiationInterventionType">
		<xs:sequence>
			<xs:element type="xs:byte" name="dosage" />
			<xs:element type="xs:string" name="dosageUnit" />
			<xs:element type="xs:date" name="lastTreatmentDate" />
			<xs:element type="xs:byte" name="fractionNumber" />
			<xs:element type="xs:byte" name="daysElapsed" />
			<xs:element type="xs:string" name="adjustment" />
			<xs:element type="xs:string" name="administration" />
			<xs:element type="OtherInterventionType" name="OtherIntervention" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InterventionSiteType">
		<xs:sequence>
			<xs:element type="xs:string" name="name" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SurgeryInterventionType">
		<xs:sequence>
			<xs:element type="xs:date" name="interventionDate" />
			<xs:element type="InterventionSiteType" name="InterventionSite" />
			<xs:element type="OtherInterventionType" name="OtherIntervention" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BiologicalInterventionType">
		<xs:sequence>
			<xs:element type="xs:string" name="description" />
			<xs:element type="OtherInterventionType" name="OtherIntervention" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DietarySupplementInterventionType">
		<xs:sequence>
			<xs:element type="xs:string" name="description" />
			<xs:element type="OtherInterventionType" name="OtherIntervention" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OtherInterventionType">
		<xs:sequence>
			<xs:element type="xs:string" name="name" />
			<xs:element type="xs:string" name="description" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeviceType">
		<xs:sequence>
			<xs:element type="xs:string" name="brandName" />
			<xs:element type="xs:string" name="commonName" />
			<xs:element type="xs:string" name="type" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="StudyDeviceType">
        <xs:complexContent>
            <xs:extension base="cct:MutableDomainObjectType">
            	<xs:choice>
            		<xs:sequence>
						<xs:element name="otherBrandName" type="xs:string"/>
                    	<xs:element name="otherCommonName" type="xs:string"/>
                    	<xs:element name="otherDeviceType" type="xs:string"/>
					</xs:sequence>
					<xs:sequence>
						<xs:element name="device" type="DeviceType" minOccurs="0"/>
	                    <xs:element name="catalogNumber" type="xs:string" minOccurs="0"/>
	                    <xs:element name="manufacturerName" type="xs:string" minOccurs="0"/>
	                    <xs:element name="manufacturerCity" type="xs:string" minOccurs="0"/>
	                    <xs:element name="manufacturerState" type="xs:string" minOccurs="0"/>
	                    <xs:element name="modelNumber" type="xs:string" minOccurs="0"/>
					</xs:sequence>
            	</xs:choice>
            </xs:extension>
    </xs:complexContent>
    
    </xs:complexType>
	<xs:complexType name="MedicalDeviceType">
		<xs:sequence>
			<xs:element type="xs:string" name="brandName" />
			<xs:element type="xs:string" name="commonName" />
			<xs:element type="xs:string" name="deviceType" />
			<xs:element type="xs:string" name="manufacturerName" />
			<xs:element type="xs:string" name="manufacturerCity" />
			<xs:element type="xs:string" name="manufacturerState" />
			<xs:element type="xs:int" name="modelNumber" />
			<xs:element type="xs:string" name="catalogNumber" />
			<xs:element type="xs:byte" name="serialNumber" />
			<xs:element type="xs:short" name="otherNumber" />
			<xs:element type="xs:date" name="explantedDate" />
			<xs:element type="xs:string" name="DeviceOperator" />
			<xs:element type="xs:string" name="DeviceReprocessed" />
			<xs:element type="xs:string" name="EvaluationAvailability" />
			<xs:element type="StudyDeviceType" name="StudyDevice" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConcomitantMedicationType">
		<xs:sequence>
			<xs:element name="name"/>
			<xs:element minOccurs="0" name="startDate" type="xs:date" />
			<xs:element minOccurs="0" name="endDate" type="xs:date" />
			<xs:element name="stillTakingMedications" type="xs:boolean"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OtherCauseType">
		<xs:sequence>
			<xs:element name="text">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Diabetes mellitus" />
						<xs:enumeration value="Bronchospasm" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DoseType">
		<xs:sequence>
			<xs:element name="amount">
				<xs:simpleType>
					<xs:restriction base="xs:short">
						<xs:enumeration value="2" />
						<xs:enumeration value="454" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="units">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="BVP" />
						<xs:enumeration value="g" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CourseAgentType">
		<xs:sequence>
			<xs:element name="lastAdministeredDate" type="xs:date" />
			<xs:element name="totalDoseAdministeredThisCourse" type="xs:decimal" />
			<xs:element name="administrationDelayAmount" type="xs:decimal" />
			<xs:element name="administrationDelayUnits" type="xs:string" />
			<xs:element name="durationAndSchedule" type="xs:string" />

			<xs:element name="Dose" type="Dose" />
			<xs:element name="ModifiedDose" type="Dose" />

			<xs:element name="StudyAgent" type="StudyAgentType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Dose">
		<xs:sequence>
			<xs:element name="amount" type="xs:string" />
			<xs:element name="units" type="xs:string" />
			<xs:element name="route" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="TreatmentInformation">
		<xs:sequence>
			<xs:element name="firstCourseDate" type="xs:date" />
			<xs:element name="CourseDate" type="CourseDate" />
			<xs:element name="CourseAgent" type="CourseAgentType"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CourseDate">
		<xs:sequence>
			<xs:element name="number" type="xs:integer" />
			<xs:element name="date" type="xs:date" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="OtherCause">
		<xs:sequence>
			<xs:element name="text" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SurgeryAttributionType">
		<xs:sequence>
			<xs:element type="AttributionType" name="attribution"/>
			<xs:element type="SurgeryInterventionType" name="cause" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RadiationAttributionType">
		<xs:sequence>
			<xs:element name="attribution" type="xs:string" />
			<xs:element name="cause" type="RadiationInterventionType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeviceAttributionType">
		<xs:sequence>
			<xs:element name="attribution" type="xs:string" />
			<xs:element name="cause" type="MedicalDeviceType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BiologicalInterventionAttributionType">
		<xs:sequence>
			<xs:element name="attribution" type="xs:string" />
			<xs:element name="cause" type="BiologicalInterventionType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DietarySupplementInterventionAttributionType">
		<xs:sequence>
			<xs:element name="attribution" type="xs:string" />
			<xs:element name="cause" type="DietarySupplementInterventionType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ConcomitantMedicationAttributionType">
		<xs:sequence>
			<xs:element name="attribution" type="xs:string" />
			<xs:element name="cause" type="ConcomitantMedicationType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CourseAgentAttributionType">
		<xs:sequence>
			<xs:element name="attribution" type="xs:string" />
			<xs:element name="cause" type="CourseAgentType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OtherCauseAttributionType">
		<xs:sequence>
			<xs:element name="attribution" type="xs:string" />
			<xs:element name="cause" type="OtherCauseType" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ParticipantHistoryType">
		<xs:sequence>
			<xs:element name="weight" type="WeightType" />
			<xs:element name="height" type="HeightType" />
			<xs:element name="baselinePerformanceStatus" type="xs:string" />
			<xs:element name="heightUnitOfMeasure" type="xs:string" />
			<xs:element name="weightUnitOfMeasure" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CtcTermType">
		<xs:sequence>
			<xs:element name="term" type="xs:string" />
			<xs:element name="select" type="xs:string" />
			<xs:element name="ctepTerm" type="xs:string" />
			<xs:element name="ctepCode" type="xs:integer" />
			<xs:element name="otherRequired" type="xs:boolean" />
