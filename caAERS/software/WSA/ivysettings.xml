<ivysettings>
	<settings defaultResolver="wsa-resolver" validate="false" />

	<resolvers>
		<chain name="wsa-resolver" returnFirst="true" dual="true">
		
            <!-- Local Ivy Cache -->
            <filesystem name="ivy-cache" local="true" >
                  <ivy pattern="${user.home}/.ivy2/wsaCache/[organisation]/[module]/ivy-[revision].xml"/>
                  <artifact pattern="${user.home}/.ivy2/wsaCache/[organisation]/[module]/[type]s/[artifact]-[revision].[ext]"/>
            </filesystem>

            <url name="spring-repo-nightly-snapshots" m2compatible="true">
                <ivy pattern="http://static.springframework.org/maven2-snapshots/[organisation]/[module]/ivy-[revision].xml"/>
                <artifact pattern="http://static.springframework.org/maven2-snapshots/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
            </url>

            <ibiblio name="mvn.repo" root="http://repo1.maven.org/maven2/" m2compatible="true" changingPattern=".*SNAPSHOT" />
            <ibiblio name="ibiblio" root="http://mirrors.ibiblio.org/pub/mirrors/maven2/" m2compatible="true"/>
            <ibiblio name="maven2-codehaus-mule" m2compatible="true" root="http://dist.codehaus.org/mule/dependencies/maven2" changingPattern=".*SNAPSHOT" />
            <ibiblio name="maven2-codehaus" m2compatible="true" root="http://repository.codehaus.org" changingPattern=".*SNAPSHOT"/>
            <ibiblio name="maven2-http" m2compatible="true" root="http://repository.jboss.com/maven2" changingPattern=".*SNAPSHOT"/>
            <ibiblio name="openqa" m2compatible="true" root="http://archiva.openqa.org/repository/releases/" changingPattern=".*SNAPSHOT"/>
            <ibiblio name="java.net" root="http://download.java.net/maven/1/" pattern="[organization]/jars/[module]-[revision].[ext]" />
            <ibiblio name="java.net2" root="http://download.java.net/maven/2/" m2compatible="true"/>

            <!--  Semanticbits shared repository [using http] -->

            <url name="semanticbits-repo-http">
                <ivy pattern="http://repo.semanticbits.com/ivy-repo/[organisation]/[module]/[revision]/ivy.xml" />
                  <artifact pattern="http://repo.semanticbits.com/ivy-repo/[organisation]/[module]/[revision]/[artifact].[ext]" />
            </url>


            <url name="semanticbits-caaers-repo-http">
                <ivy pattern="http://repo.semanticbits.com/ivy-repo/caAERS/[organisation]/[module]/ivy-[revision].xml" />
                  <artifact pattern="http://repo.semanticbits.com/ivy-repo/caAERS/[organisation]/[module]/jars/[artifact]-[revision].[ext]" />
            </url>


                <!--  Northwestern RHLCCC Bioinformatics  -->
                <ibiblio name="maven2-northwestern" m2compatible="true" root="http://download.bioinformatics.northwestern.edu/download/maven2" changingPattern=".*SNAPSHOT" />
            
                <ibiblio name="maven2-http" m2compatible="true" root="http://maven2.mirrors.skynet.be/pub/maven2" changingPattern=".*SNAPSHOT"/>
                <ibiblio name="mvn.repo" root="http://mirrors.ibiblio.org/pub/mirrors/maven2" m2compatible="true" changingPattern=".*SNAPSHOT" />
                <ibiblio name="download-java-net" m2compatible="true" root="http://download.java.net/maven/2" changingPattern=".*SNAPSHOT" />
                <ibiblio name="maven-repo2"  m2compatible="true" root="http://repo2.maven.org/maven2"	changingPattern=".*SNAPSHOT" />


                <url name="com.springsource.repository.bundles.release">
                    <ivy pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                    <artifact pattern="http://repository.springsource.com/ivy/bundles/release/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                </url>
                <url name="com.springsource.repository.bundles.external">
                    <ivy pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                    <artifact pattern="http://repository.springsource.com/ivy/bundles/external/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                </url>

                <!--  Semanticbits shared caGRID repository [using http] -->
                <url name="semanticbits-grid-repo-http">
                    <ivy pattern="http://repo.semanticbits.com/ivy-repo/caGRID/[organisation]/[module]/ivy-[revision].xml" />
                      <artifact pattern="http://repo.semanticbits.com/ivy-repo/caGRID/[organisation]/[module]/[revision]/[artifact].[ext]" />
                      <artifact pattern="http://repo.semanticbits.com/ivy-repo/caGRID/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
                      <artifact pattern="http://repo.semanticbits.com/ivy-repo/caGRID/[organisation]/[module]/[revision]/[organization]-[artifact]-[revision].[ext]" />
                      <artifact pattern="http://repo.semanticbits.com/ivy-repo/caGRID/[organisation]/[module]/[revision]/[organization]-[module]-[artifact]-[revision].[ext]" />
                      <artifact pattern="http://repo.semanticbits.com/ivy-repo/caGRID/[organisation]/[module]/[revision]/[artifact].[ext]" />
                </url>

        </chain>

	</resolvers>
</ivysettings>

