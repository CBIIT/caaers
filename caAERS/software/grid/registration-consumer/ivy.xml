			<dependency org="org.projectmobius" name="mobius-gme-client" rev="1.1" />
			<dependency org="org.projectmobius" name="mobius-mako-client" rev="1.1" />
			<dependency org="org.projectmobius" name="mobius-tools" rev="1.1" />
