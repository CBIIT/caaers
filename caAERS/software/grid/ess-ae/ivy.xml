				conf="build->publish;runtime->publish,runtime;dist->dist;distGrid->distGrid"/>
			<dependency org="gov.nih.nci.cabig.caaers" name="caaers-web" rev="latest.integration" 
				conf="build->publish;runtime->publish,runtime;dist->dist;distGrid->distGrid"/>
				
