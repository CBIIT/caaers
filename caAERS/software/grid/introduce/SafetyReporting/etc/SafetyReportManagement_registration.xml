<?xml version="1.0" encoding="UTF-8" ?>
<ServiceGroupRegistrationParameters xmlns:sgc="http://mds.globus.org/servicegroup/client"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:wsa="http://schemas.xmlsoap.org/ws/2004/03/addressing"
  xmlns:agg="http://mds.globus.org/aggregator/types"
  xmlns="http://mds.globus.org/servicegroup/client">
  <!-- The ServiceGroupEPR defines the servicegroup to which registrations will be made -->
  <ServiceGroupEPR>
    <wsa:Address>INDEX_SERVICE_URL</wsa:Address>
  </ServiceGroupEPR>
  <!-- Specifies that the registration will be renewed every 10 minutes -->
  <RefreshIntervalSecs>REGISTRATION_REFRESH</RefreshIntervalSecs>
  <Content xsi:type="agg:AggregatorContent" xmlns:agg="http://mds.globus.org/aggregator/types">
    <agg:AggregatorConfig xsi:type="agg:AggregatorConfig">
      <agg:GetMultipleResourcePropertiesPollType
			xmlns:ns13="http://service.sr.ess.nci.nih.gov/SafetyReporting/SafetyReportNotification/types"
			xmlns:ns16="http://service.sr.ess.nci.nih.gov/SafetyReporting/SafetyReportRulesEvaluation/types"
			xmlns:ns0="http://ess.caaers.nci.nih.gov/sr"
			xmlns:ns8="http://service.sr.ess.nci.nih.gov/SafetyReporting/SafetyReportQuery/types"
			xmlns:ns1="uri:iso.org:21090"
			xmlns:ns6="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.service"
			xmlns:ns14="http://service.sr.ess.nci.nih.gov/SafetyReporting/SafetyReportReview/types"
			xmlns:ns4="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"
			xmlns:ns10="http://service.sr.ess.nci.nih.gov/SafetyReporting/SafetyReportManagement/types"
			xmlns:ns11="http://service.sr.ess.nci.nih.gov/SafetyReporting/SafetyReportDefinitionManagement/types"
			xmlns:ns17="http://service.sr.ess.nci.nih.gov/SafetyReporting/SafetyReportDefinitionQuery/types"
			xmlns:ns2="http://ess.caaers.nci.nih.gov"
			xmlns:ns5="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"
			xmlns:ns7="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.common"
			xmlns:ns15="http://service.sr.ess.nci.nih.gov/SafetyReporting/SafetyReportAdvancedQuery/types"
			xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:ns18="http://service.sr.ess.nci.nih.gov/SafetyReporting/SafetyReportTransaction/types"
			xmlns:ns3="http://service.sr.ess.nci.nih.gov/SafetyReporting/types"
			xmlns:ns9="http://docs.oasis-open.org/wsrf/2004/06/wsrf-WS-ResourceLifetime-1.2-draft-01.xsd"
			xmlns:ns12="http://service.sr.ess.nci.nih.gov/SafetyReporting/SafetyReportRulesManagement/types">
        <!-- Specifies that the index should refresh information
            every 300000 milliseconds (once every 5 minutes) -->
        <agg:PollIntervalMillis>INDEX_REFRESH</agg:PollIntervalMillis>
        <!-- specifies all Resource Properties that should be retrieved from the service -->
      </agg:GetMultipleResourcePropertiesPollType>
    </agg:AggregatorConfig>
    <agg:AggregatorData/>
  </Content>
</ServiceGroupRegistrationParameters>
