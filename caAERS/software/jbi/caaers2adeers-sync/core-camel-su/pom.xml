      </dependency>
      <dependency>
          <groupId>org.apache.servicemix</groupId>
          <artifactId>servicemix-cxf-bc</artifactId>
          <classifier>installer</classifier>
          <type>zip</type>
          <version>${cxfBCVersion}</version>
          <scope>test</scope>
      </dependency>
      <dependency>
          <groupId>org.apache.servicemix</groupId>
