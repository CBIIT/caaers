      <svrl:text>Unavailable matching autopsy flag</svrl:text>
   </svrl:failed-assert>
   <svrl:active-pattern document="file:/Users/vinodh/sb/projects/caaers/caAERS/software/jbi/caaers2adeers-sync/core-camel-su/target/test-classes/xslt/e2b/request/testXMLs/e2b_lookups_sch_validation_input.xml"
                        id="ECOG or ZUBROD basline performance scale"
                        name="BaselinePerformance: validate baseline performance number"/>
   <svrl:fired-rule context="/ichicsr/safetyreport/patient[upper-case(baselineperformancescale)='ECOG' or upper-case(baselineperformancescale)='ZUBROD']"/>
