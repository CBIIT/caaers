   <svrl:fired-rule context="/ichicsr/safetyreport/patient/summary/retreatedflag"/>
   <svrl:failed-assert test="." location="/ichicsr/safetyreport/patient/summary/retreatedflag">
      <svrl:text>Unavailable matching retreated flag</svrl:text>
   </svrl:failed-assert>
   <svrl:active-pattern document="file:/Users/vinodh/sb/projects/caaers/caAERS/software/jbi/caaers2adeers-sync/core-camel-su/target/test-classes/xslt/e2b/request/testXMLs/e2b_lookups_sch_validation_input.xml"
                        id="Patient Death"
                        name="Patient Death: validate autopsy flag"/>
   <svrl:fired-rule context="/ichicsr/safetyreport/patient/patientdeath/patientautopsyyesno"/>
