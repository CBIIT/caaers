                        id="Course Agent"
                        name="CourseAgent: validate dose unit"/>
   <svrl:fired-rule context="//drug[drugadditional != 'Radiation' and  drugadditional != 'Surgery' and drugadditional != 'Device' and drugcharacterization = '1']"/>
   <svrl:failed-assert test="drugcumulativedosageunit"
                       location="/ichicsr/safetyreport/patient/drug[2]">
      <svrl:text>Unavailable matching course agent dose unit</svrl:text>
   </svrl:failed-assert>
   <svrl:active-pattern document="file:/Users/vinodh/sb/projects/caaers/caAERS/software/jbi/caaers2adeers-sync/core-camel-su/target/test-classes/xslt/e2b/request/testXMLs/e2b_lookups_sch_validation_input.xml"
                        id="Lab"
                        name="Lab: validate lab test unit"/>
   <svrl:active-pattern document="file:/Users/vinodh/sb/projects/caaers/caAERS/software/jbi/caaers2adeers-sync/core-camel-su/target/test-classes/xslt/e2b/request/testXMLs/e2b_lookups_sch_validation_input.xml"
                        id="Patient Summary"
